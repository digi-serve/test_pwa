{
   "name": "appbuilder-pwa",
   "private": true,
   "version": "1.0.5",
   "description": "AppBuilder PWA",
   "repository": "",
   "license": "UNLICENSED",
   "scripts": {
      "start": "npm run dev",
      "dev": "cross-env NODE_ENV=development vite",
      "build_main": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
      "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
      "proxy": "./proxy.sh",
      "watch": "webpack-cli --config webpack.dev.js --watch --progress",
      "build": "webpack-cli --config webpack.dev.js --progress",
      "build:update": "npm run postinstall && webpack-cli --config webpack.dev.js --progress",
      "build:dev": "git submodule update --init --recursive && webpack-cli --config webpack.dev.js --progress"
   },
   "browserslist": [
      "IOS >= 15",
      "Safari >= 15",
      "last 5 Chrome versions",
      "last 5 Firefox versions"
   ],
   "dependencies": {
      "atomicjs": "^4.4.1",
      "dom7": "^4.0.6",
      "eventemitter2": "^6.4.9",
      "framework7": "^8.0.5",
      "framework7-icons": "^5.0.5",
      "lodash": "^4.17.21",
      "material-icons": "^1.13.6",
      "moment": "^2.29.4",
      "nanoid": "^4.0.2",
      "papaparse": "^5.5.3",
      "semver": "^7.7.2",
      "skeleton-elements": "^4.0.1",
      "swiper": "^9.3.2"
   },
   "devDependencies": {
      "@babel/core": "^7.22.9",
      "@babel/plugin-syntax-jsx": "^7.22.5",
      "@babel/preset-env": "^7.22.9",
      "@babel/preset-react": "^7.22.5",
      "babel-loader": "^9.1.3",
      "clean-webpack-plugin": "^4.0.0",
      "compression-webpack-plugin": "^10.0.0",
      "cpy-cli": "^4.2.0",
      "cross-env": "^7.0.3",
      "css-loader": "^5.0.1",
      "eslint": "^8.21.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.2.1",
      "framework7-loader": "^3.0.2",
      "html-loader": "^4.2.0",
      "html-webpack-plugin": "^5.5.0",
      "less": "^4.1.3",
      "less-loader": "^11.1.3",
      "local-cors-proxy": "^1.1.0",
      "postcss-preset-env": "^8.3.2",
      "prettier": "^2.2.1",
      "rollup-plugin-framework7": "^1.2.1",
      "style-loader": "^2.0.0",
      "url-loader": "^4.1.1",
      "vite": "^6.3.5",
      "webpack": "^5.74.0",
      "webpack-cli": "^4.10.0",
      "webpack-merge": "^5.8.0",
      "workbox-cli": "^6.5.4"
   }
}
